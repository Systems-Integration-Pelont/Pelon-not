{
  "AllowedHosts": "*",
  "AllowedOrigins": "",
  "ConnectionStrings": {
    "Database": "",
    "Redis": ""
  },
  "Cache": {
    "InstanceName": "prod",
    "DefaultDurationMinutes": 10
  },
  "HealthChecks": {
    "Enabled": true
  },
  "RateLimiter": {
    "Key": "GlobalKey",
    "PermitLimit": 150,
    "WindowInSeconds": 60
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [{ "Name": "Console" }],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  },
  "Jwt": {
    "Secret": "",
    "Issuer": "",
    "Audience": "",
    "ExpirationInMinutes": 0
  },
  "Consul": {
    "Id": "authentication-service-",
    "Name": "AuthenticationService"
  },
  "ServiceDiscovery": {
    "Url": "",
    "Port": 8080,
    "ApiHost": ""
  }
}
